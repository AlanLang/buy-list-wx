<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">物品名称</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" type="text" disabled="{{true}}" value="{{formData.BuyName}}"></input>
    </view>
  </view>

  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">物品价格</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" type="text" disabled="{{true}}" value="{{formData.BuyPrice}}"></input>
    </view>
  </view>

  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">购买地址</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" type="text" disabled="{{true}}" value="{{formData.BuyUrl}}"></input>
    </view>
  </view>

    <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">物品类别</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" type="text" disabled="{{true}}" value="{{formData.BuyTypeName}}"></input>
    </view>
  </view>
    <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">需求等级</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" type="text" disabled="{{true}}" value="{{formData.BuyLevel}}"></input>
    </view>
  </view>
    <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">提交人</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" type="text" disabled="{{true}}" value="{{formData.BuyAuthor}}"></input>
    </view>
  </view>
      <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">提交时间</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" type="text" disabled="{{true}}" value="{{formData.BuyTime}}"></input>
    </view>
  </view>
  <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
      <view class="weui-label">描述</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" type="text" disabled="{{true}}" value="{{formData.BuyDesc}}"></input>
    </view>
  </view>

</view>
  <view class='button-group btn-area'>
    <button bindtap='bindAgree'  type="primary">同意</button>
    <button bindtap='bindDisagree' class='btn-other'  type="warn">不同意</button>
  </view>